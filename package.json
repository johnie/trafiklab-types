{
    "name": "trafiklab-types",
    "type": "module",
    "version": "1.0.0",
    "description": "TypeScript definitions for Trafiklab API",
    "main": "dist/index.d.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*.d.ts"
    ],
    "scripts": {
        "build": "tsc --emitDeclarationOnly --declaration",
        "ci": "$npm_execpath run build && $npm_execpath run check && $npm_execpath run test",
        "prepublishOnly": "bun run check && bun run build",
        "check": "biome check --write",
        "test": "vitest run"
    },
    "keywords": [
        "trafiklab",
        "typescript",
        "types"
    ],
    "author": "Johnie Hjelm <johnie@hjelm.im>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/johnie/trafiklab-types.git"
    },
    "bugs": {
        "url": "https://github.com/johnie/trafiklab-types/issues"
    },
    "homepage": "https://github.com/johnie/trafiklab-types#readme",
    "devDependencies": {
        "@biomejs/biome": "2.2.2",
        "@types/node": "^24.3.0",
        "msw": "2.10.5",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "typescript": "^5"
    }
}
